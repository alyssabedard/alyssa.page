---
// Main Layout

interface Props {
	title: string;
}
const { title } = Astro.props;

import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
import MainHead from './MainHead.astro';
---

<!doctype html>
<html lang="en">
	<MainHead title={title}/>
	<header></header>
	<nav>
		<Navigation />
	</nav>
	<body>
		<slot />
	</body>
	<footer>
		<Footer />
	</footer>
</html>

